{
  "name": "morse-ui",
  "version": "0.0.1",
  "description": "> TODO: description",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "babel.js",
    "package.json"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/moyongshi/morse-ui.git"
  },
  "author": "hesong gao <hs_gao@trip.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moyongshi/morse-ui/issues"
  },
  "homepage": "https://github.com/moyongshi/morse-ui/blob/master/README.md",
  "keywords": [
    "ui",
    "react",
    "react-native",
    "morse-react-native"
  ],
  "scripts": {
    "prebuild": "rimraf lib && npm run icon-build",
    "build": "bob build",
    "devBuild": "yarn build && yalc push",
    "icon-build": "rimraf src/components/Icon && iconfont-rn",
    "dev": "onchange 'src/**/*.ts' 'src/**/*.tsx' -- yarn devBuild",
    "prepublishOnly": "yarn build",
    "yalcPublish": "yalc publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "commit": "git cz"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/eslint-config": "^12.0.1",
    "@commitlint/cli": "^12.1.4",
    "@types/color": "^3.0.2",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "core-js": "^3.15.2",
    "cz-customizable": "^6.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "7",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "lerna": "^4.0.0",
    "lint-staged": "^12.1.4",
    "metro-react-native-babel-preset": "^0.64.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "patch-package": "^6.4.7",
    "plop": "^3.0.5",
    "pod-install": "^0.1.24",
    "react-native-iconfont-cli": "^2.2.4",
    "react-native-mock-render": "^0.1.9",
    "react-test-renderer": "17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@emotion/native": "^11.0.0",
    "ahooks": "^2.10.11",
    "color": "^4.0.1",
    "expo-constants": "^11.0.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "^0.65.1",
    "react-native-builder-bob": "^0.18.2",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-svg": "^12.1.1",
    "styled-system": "^5.1.5",
    "theming": "^3.3.0"
  },
  "peerDependencies": {
    "react": ">=16.5",
    "react-native": ">=0.64",
    "react-native-vector-icons": "*"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ],
    "files": [
      "src/"
    ]
  }
}
